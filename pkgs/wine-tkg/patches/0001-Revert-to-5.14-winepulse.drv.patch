-static NTSTATUS pulse_release_stream(void *args)
-{
-    struct release_stream_params *params = args;
-    struct pulse_stream *stream = params->stream;
-
-    if(params->timer) {
-        stream->please_quit = TRUE;
-        NtWaitForSingleObject(params->timer, FALSE, NULL);
