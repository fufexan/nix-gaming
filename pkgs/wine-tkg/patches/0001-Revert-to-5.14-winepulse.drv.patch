-            p += i;
-        } while ((BYTE*)p != end);
-        break;
-    }
-    case PA_SAMPLE_ULAW:
-    {
-        UINT8 *p = (UINT8*)buffer;
-        do
-        {
-            for (i = 0; i < channels; i++)
