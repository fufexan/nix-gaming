-    if (written_bytes <= chunk_bytes)
-    {
-        memcpy(stream->local_buffer + wri_offs_bytes, buffer, written_bytes);
-    }
-    else
-    {
-        memcpy(stream->local_buffer + wri_offs_bytes, buffer, chunk_bytes);
